{
  "name": "doloresnihil",
  "version": "1.0.0",
  "description": "Mocking library for TypeScript",
  "main": "lib/ts-mockito.js",
  "typings": "lib/ts-mockito",
  "author": "DarioAltenwerth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MadgeNolan/doloresnihil.git"
  },
  "keywords": [
    "voluptate",
    "typescript",
    "quibusdam",
    "fake",
    "stub",
    "natus",
    "javascript"
  ],
  "devDependencies": {
    "@types/jasmine": "^2.6.0",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.104",
    "@types/node": "^8.0.46",
    "babel-polyfill": "^6.26.0",
    "jasmine-core": "^2.8.0",
    "jest": "^23.5.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^3.0.8",
    "karma-typescript-preprocessor": "^0.3.1",
    "phantomjs-prebuilt": "^2.1.16",
    "puppeteer": "^0.12.0",
    "ts-helpers": "^1.1.2",
    "ts-jest": "^23.0.1",
    "tslint": "^5.7.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageReporters": [
      "lcov"
    ]
  }
}